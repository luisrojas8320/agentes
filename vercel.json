{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/next.config.mjs"
    }
  ]
}